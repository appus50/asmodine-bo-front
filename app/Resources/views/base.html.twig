        })(window,document,'script','dataLayer','GTM-NXRPMPP');</script>
    <!-- End Google Tag Manager -->
    {% block stylesheets %}
        {% stylesheets '@thirdparty_css' output='css/asmodine.vendors.min.css' %}
            <link rel="stylesheet" href="{{ preload(asset(asset_url), { as: 'style' }) }}" />
        {% endstylesheets %}
        {% stylesheets '@asmodine_css' output='css/asmodine.app.min.css' %}
            <link rel="stylesheet" href="{{ preload(asset(asset_url), { as: 'style' }) }}" />
        {% endstylesheets %}
        {% stylesheets '@asmodine_scss' output='css/asmodine.base.min.css' %}
            <link rel="stylesheet" href="{{ preload(asset(asset_url), { as: 'style' }) }}" />
        {% endstylesheets %}
        <link href='{{ preload(asset('https://fonts.googleapis.com/css?family=Oswald:400,700,300'), { as: 'style' }) }}' rel='stylesheet' type='text/css'>
        <link href='{{ preload(asset('https://fonts.googleapis.com/css?family=Roboto+Condensed'), { as: 'style' }) }}' rel='stylesheet' type='text/css'>
    {% endblock %}
    <!-- Facebook Pixel Code -->
    <script>
        !function(f,b,e,v,n,t,s)
        {if(f.fbq)return;n=f.fbq=function(){n.callMethod?
            n.callMethod.apply(n,arguments):n.queue.push(arguments)};
            if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version='2.0';
            n.queue=[];t=b.createElement(e);t.async=!0;
            t.src=v;s=b.getElementsByTagName(e)[0];
            s.parentNode.insertBefore(t,s)}(window,document,'script', 'https://connect.facebook.net/en_US/fbevents.js');
        fbq('init', '{{ facebook_px_id }}');
        fbq('track', 'PageView');
    </script>
    <noscript>
        <img height="1" width="1" src="https://www.facebook.com/tr?id={{ facebook_px_id }}&ev=PageView&noscript=1"/>
    </noscript>
    <!-- End Facebook Pixel Code -->
    <!-- Mailchimp -->
    <script id="mcjs">!function(c,h,i,m,p){m=c.createElement(h),p=c.getElementsByTagName(h)[0],m.async=1,m.src=i,p.parentNode.insertBefore(m,p)}(document,"script","https://chimpstatic.com/mcjs-connected/js/users/80132c6bb5a3de15d425bc6ab/057e0b130d843e9e4bc5a419e.js");</script>
    <!-- Endmailchimp -->
    <!-- TradeDoubler site verification 3023391 -->
</head>
<body class="{% if app.user %}user-connect {% endif %}{% block body_class %}{% endblock %}">
<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NXRPMPP" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
{% block fullbody %}
    {{ render(url('asmodinefront_fragment_social')) }}
    {{ include('@AsmodineFrontBundle/Fragment/header.html.twig') }}

    {% block body %}{% endblock %}

    {{ render(url('asmodinefront_fragment_footer')) }}

    {% include 'AsmodineFrontBundle:Fragment:modals.html.twig' %}
    <!-- Older IE warning message -->
    <!--[if lt IE 9]>
    <p>{{ 'main.ltie9'|trans }}</p>
    <![endif]-->
{% endblock %}
{% block javascripts %}
    {% javascripts '@thirdparty_js' filter='?jsqueeze' output='js/asmodine.vendors.min.js' %}
        <script src="{{ preload(asset(asset_url), { as: 'script' }) }}"></script>
    {% endjavascripts %}
    {% javascripts '@asmodine_js' filter='?jsqueeze' output='js/asmodine.base.min.js' %}
        <script src="{{ preload(asset(asset_url), { as: 'script' }) }}"></script>
    {% endjavascripts %}
    {% javascripts filter="?jsqueeze" output="js/jquery.placeholder.min.js"
        "@AsmodineFrontBundle/Resources/public/thirdparty/js/jquery.placeholder.js" %}
        <!--[if IE 9 ]><script src="{{ preload(asset(asset_url), { as: 'script' }) }}}"></script><![endif]-->
    {% endjavascripts %}
{% endblock %}
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
    ga('create', '{{ ga_id }}', 'auto');
    ga('send', 'pageview');
</script>
</body>
</html>